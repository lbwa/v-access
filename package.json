{
  "name": "v-access",
  "version": "2.0.0",
  "description": "An authorization solutions for Vue.js v2.x.",
  "main": "dist/v-access.cjs.development.js",
  "module": "dist/v-access.esm.development.js",
  "types": "dist/install.d.ts",
  "unpkg": "dist/v-access.production.min.js",
  "jsdelivr": "dist/v-access.production.min.js",
  "repository": "git@github.com:lbwa/v-access.git",
  "author": "Bowen <vnode@gmail.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/lbwa/v-access/issues"
  },
  "homepage": "https://github.com/lbwa/v-access#readme",
  "scripts": {
    "clean": "rm -rf dist/*",
    "build": "npm run clean && npm run build:all && cp -p src/dep.d.ts dist/dep.d.ts",
    "build:all": "yarn rollup --config build/rollup.config.js",
    "test": "jest --coverage"
  },
  "dependencies": {
    "tiny-invariant": "^1.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/jest": "^25.1.3",
    "cross-env": "^7.0.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rollup": "^1.31.1",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "keywords": [
    "access-control",
    "v-access",
    "vue-access",
    "vue-plugins",
    "web",
    "typescript",
    "vue",
    "vue-router",
    "authorization",
    "vue-auth",
    "vue-authorization",
    "vuejs"
  ],
  "jest": {
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "\\\\node_modules\\\\"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ],
    "preset": "ts-jest",
    "roots": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "\\\\node_modules\\\\",
      "\\\\examples\\\\",
      "\\\\dist\\\\"
    ],
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    }
  }
}
