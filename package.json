{
  "name": "v-access",
  "version": "0.3.0",
  "description": "An authorization solutions for Vue v2.x.",
  "main": "dist/v-access.min.js",
  "module": "dist/v-access.esm.js",
  "types": "dist/src/index.d.ts",
  "unpkg": "dist/v-access.min.js",
  "jsdelivr": "dist/v-access.min.js",
  "repository": "git@github.com:lbwa/v-access.git",
  "author": "Bowen <asnycmode@gmail.com>",
  "license": "MIT",
  "private": false,
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/lbwa/v-access/issues"
  },
  "homepage": "https://github.com/lbwa/v-access#readme",
  "scripts": {
    "clean": "rm -vrf dist/*",
    "serve:router": "yarn build && cd examples/with-router && yarn serve",
    "build": "npm run clean && npm run build:umd && npm run build:es",
    "build:umd": "cross-env TARGET=umd rollup --config build/rollup.config.js",
    "build:es": "cross-env TARGET=es rollup --config build/rollup.config.js",
    "test": "jest --coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "cross-env": "^5.2.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "rollup": "^1.19.4",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.22.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3",
    "vue": "^2.6.10",
    "vue-router": "^3.1.2"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "keywords": [
    "access-control",
    "v-access",
    "vue-access",
    "vue-plugins",
    "web",
    "typescript",
    "vue",
    "vue-router",
    "authorization",
    "vue-auth",
    "vue-authorization"
  ],
  "jest": {
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "\\\\node_modules\\\\"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ],
    "preset": "ts-jest",
    "roots": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "\\\\node_modules\\\\",
      "\\\\examples\\\\",
      "\\\\dist\\\\"
    ],
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    }
  }
}
